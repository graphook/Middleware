{
  "name": "zenow-middleware",
  "version": "1.0.0",
  "description": "The middleware for Zenow",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run start",
    "build": "babel src --out-dir build",
    "dev": "better-npm-run dev",
    "test": "mocha \"src/**/*.spec.js\" --compilers js:babel-register",
    "coverage": "babel-node node_modules/.bin/babel-istanbul cover _mocha -- \"src/**/*.spec.js\"",
    "postinstall": "npm run build",
    "initMongo": "better-npm-run initMongo",
    "initES": "better-npm-run initES"
  },
  "betterScripts": {
    "start": {
      "command": "node ./build/index.js",
      "env": {
        "NODE_PATH": "./build"
      }
    },
    "dev": {
      "command": "babel-watch src/index.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "dev",
        "PORT": 3030
      }
    },
    "initMongo": {
      "command": "babel-node src/initMongo.js",
      "env": {
        "NODE_PATH": "./src"
      }
    },
    "initES": {
      "command": "babel-node src/initES.js",
      "env": {
        "NODE_PATH": "./src"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphook/Middleware.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/graphook/Middleware/issues"
  },
  "homepage": "https://github.com/graphook/Middleware#readme",
  "devDependencies": {
    "babel-istanbul": "^0.11.0",
    "babel-watch": "^2.0.2",
    "chai": "^3.5.0"
  },
  "dependencies": {
    "async": "^2.1.4",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "bcrypt-nodejs": "0.0.3",
    "better-npm-run": "0.0.13",
    "bluebird": "^3.4.7",
    "body-parser": "^1.15.2",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "jsonschema": "^1.1.0",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.11",
    "node-uuid": "^1.4.7",
    "superagent": "^2.3.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
