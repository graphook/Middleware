{
  "name": "graphook-middleware",
  "version": "1.0.0",
  "description": "The middleware for Graphook",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "build": "babel src --out-dir build",
    "dev": "babel-watch src/index.js",
    "test": "mocha \"src/**/*.spec.js\" --compilers js:babel-register",
    "coverage": "babel-node node_modules/.bin/babel-istanbul cover _mocha -- \"src/**/*.spec.js\"",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/graphook/Middleware.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/graphook/Middleware/issues"
  },
  "homepage": "https://github.com/graphook/Middleware#readme",
  "devDependencies": {
    "babel-istanbul": "^0.11.0",
    "babel-watch": "^2.0.2",
    "chai": "^3.5.0"
  },
  "dependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "express": "^4.14.0",
    "webpack": "^1.13.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
